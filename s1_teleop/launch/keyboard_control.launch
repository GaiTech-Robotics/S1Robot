<launch>
  <!-- s1_teleop_key already has its own built in velocity smoother -->
  <node pkg="s1_teleop" type="s1_teleop_key" name="s1_teleop_keyboard"  output="screen">
    <param name="scale_linear" value="0.5" type="double"/>
    <param name="scale_angular" value="1.5" type="double"/>
    <remap from="s1_teleop_keyboard/cmd_vel" to="/cmd_vel"/>
  </node>
</launch>
