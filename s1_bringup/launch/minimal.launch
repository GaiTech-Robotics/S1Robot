<launch>
  <param name="/use_sim_time" value="false"/>

  <include file="$(find s1_bringup)/launch/includes/robot.launch.xml" />

 
  <!-- start python serial node -->
  <arg name="port" default="/dev/ttyUSB0" />
  <node name="serial_node" pkg="rosserial_python" type="serial_node.py" args="$(arg port)" />

  <!-- sub robot speed and pub odom -->
  <node name="subSpeedpubOdom" pkg="s1_base_controller" type="subSpeedpubOdom" />


</launch>
